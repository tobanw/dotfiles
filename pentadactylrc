"hg7226

loadplugins '\.(js|penta)$'
group user
map -count -description='pass next key as builtin' -modes=n,v <A-S-B> -builtin <count><pass-next-key-builtin>
map -count -description='delete buffer and focus previous tab' -modes=n,v d -builtin <count>D
map -count -description='delete buffer and focus next tab' -modes=n,v D -builtin <count>d
map -count -description='scroll down 6 lines' -modes=n,v j -builtin <count>6<scroll-down-line>
map -count -description='next tab' -modes=n,v J -builtin <count><C-n>
map -count -description='scroll up 6 lines' -modes=n,v k -builtin <count>6<scroll-up-line>
map -count -description='previous tab' -modes=n,v K -builtin <count><C-p>
set cdpath=''
set followhints=1
set guioptions=bCrns
set passkeys=www.google.com/search:<up><down><left><right>,mail.google.com:asdqeryuiophjklzxcvnm\,./?!#[]{}_<up><down><left><right><esc><S-t>,gs,gi,gt,gd,ga,gc,gk,gl,*a,*n,*r,*u,*s,*t,hm,ha,hi,hc,gp,hp,plus.google.com:<up><down><left><right>jkorlv,github.com:?,gn,gc,gp,gw,inbox.google.com:jkieyzrctunpqam<S-p><S-x><C-k>/?!#.[]<up><down><left><right><esc>
